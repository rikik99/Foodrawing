<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.food.global.common.mapper.GlobalCartMapper">
	<select id="getCartItemCount">
		SELECT NVL(SUM(QUANTITY), 0) AS count
		FROM CART_TB
		WHERE CUSTOMER_ID = 1
	</select>
</mapper>